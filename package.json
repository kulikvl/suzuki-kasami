{
  "name": "suzuki-kasami",
  "version": "1.0.0",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/app.ts",
    "server": "ts-node -r tsconfig-paths/register src/server.ts",
    "client": "ts-node -r tsconfig-paths/register src/client.ts",
    "prebuild": "rm -rf ./src/generated/* && npm run generate-types",
    "generate-types": "./node_modules/.bin/grpc_tools_node_protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated --ts_proto_opt=outputServices=nice-grpc,outputServices=generic-definitions,useExactTypes=false --proto_path=./src/protos ./src/protos/*.proto"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "@types/express": "^5.0.0",
    "@types/node": "^18.19.50",
    "grpc-tools": "^1.12.4",
    "prettier": "^3.3.3",
    "ts-node": "10.9.2",
    "ts-proto": "^2.2.7",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.6.3"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.12.2",
    "@grpc/proto-loader": "^0.7.13",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "google-protobuf": "^3.21.4",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "long": "^5.2.3",
    "nice-grpc": "^2.1.10",
    "protobufjs": "^7.4.0",
    "readline": "^1.3.0",
    "yaml": "^2.6.0"
  }
}
